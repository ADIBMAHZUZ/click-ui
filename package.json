{
  "name": "click",
  "description": "Cloud Library Infomation Center",
  "version": "2.10.1",
  "author": "NNG Limited Company",
  "email": "click@nng.bz",
  "homepage": "http://nng.bz",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve --open",
    "build": "ionic build",
    "build:prod": "ionic build --prod",
    "deploy:ios": "cd ios/App && gem install bundler && bundle install && bundle exec fastlane beta",
    "deploy:android": "cd android && ./gradlew assembleDebug",
    "deploy:electron-mac": "cd electron && npm i && npm run electron:build-mac",
    "deploy:electron-windows": "cd electron && npm i && npm run electron:build-windows",
    "lint": "tslint --project tsconfig.json",
    "sync": "npx cap sync",
    "e2e": "ng e2e",
    "resources": "cordova-res android --skip-config --copy && cordova-res ios --skip-config --copy",
    "build-sync": "ionic build --prod && npx cap sync",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "open:electron": "npm run build && npx cap copy && npx cap open electron",
    "open:all": "npx cap open ios && npx cap open android",
    "live-reload:ios": "ionic capacitor run ios -l --external",
    "live-reload:electron": "ionic capacitor run electron -l --external",
    "live-reload:android": "ionic capacitor run android -l --external",
    "run:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "run:android": "npm run build && npx cap sync android && npx cap open android",
    "postinstall": "jetifier"
  },
  "private": true,
